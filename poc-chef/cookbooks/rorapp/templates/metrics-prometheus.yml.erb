- module: prometheus
  metricsets:
    - stats
  period: 10s
  hosts: ["<%= @es_host %>:9090"]
  metrics_path: /metrics
  namespace: prom_metrics_stats

- module: prometheus
  metricsets:
    - collector
  period: 10s
  hosts: ["<%= @es_host %>:9090"]
  metrics_path: /metrics
  namespace: prom_metrics_collector
